.anion {
  -webkit-animation: 1.2s ani-on both cubic-bezier(0.45, 0.05, 0.55, 0.95);
          animation: 1.2s ani-on both cubic-bezier(0.45, 0.05, 0.55, 0.95);
}
@-webkit-keyframes ani-on {
0% {
    -webkit-transform: scale(0.92);
    transform: scale(0.92);
}
15% {
    -webkit-transform: scale(1.2);
    transform: scale(1.2);
}
32% {
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
}
50% {
    -webkit-transform: scale(1);
    transform: scale(1);
}
to {
    -webkit-transform: scale(1);
    transform: scale(1);
}
}
@keyframes ani-on {
0% {
    -webkit-transform: scale(0.92);
    transform: scale(0.92);
}
15% {
    -webkit-transform: scale(1.2);
    transform: scale(1.2);
}
32% {
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
}
50% {
    -webkit-transform: scale(1);
    transform: scale(1);
}
to {
    -webkit-transform: scale(1);
