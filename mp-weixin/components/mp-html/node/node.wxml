}
/**
 * @description 是否使用 rich-text 显示剩余内容
 */
module.exports = {
	gallery: function(str) {
		if (!str) return false
		return str.indexOf('wp-block-gallery') != -1;
	},
	galleryitem: function(str) {
		if (!str) return false
		return str.indexOf('wp-block-image') != -1;
	},
	video1: function(str) {
		if (!str) return false
		return str.indexOf('video1') != -1;
	},
	video16: function(str) {
		if (!str) return false
		return str.indexOf('video16') != -1;
	},
	video4: function(str) {
		if (!str) return false
		return str.indexOf('video4') != -1;
	},
	video0: function(str) {
		if (!str) return false
		return str.indexOf('video0') != -1;
	},
	use: function(item) {
		if (item.c) return false
		// 微信和 QQ 的 rich-text inline 布局无效
		return !inlineTags[item.name] && (item.attrs.style || '').indexOf('display:inline') == -1
	},
}
